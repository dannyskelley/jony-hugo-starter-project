{
  "name": "hugo-starter-project",
  "private": true,
  "scripts": {
    "prestart": "npm run build:icons",
    "start": "hugo server --buildDrafts",
    "prebuild": "npm run build:icons",
    "build": "hugo && npm run build:functions && hugo --minify --gc",
    "build:icons": "svgstore assets/svg/**/*.svg > static/sprite.svg --inline",
    "build:functions": "netlify-lambda build assets/lambda",
    "server": "hugo server"
  },
  "dependencies": {
    "autoprefixer": "^9.7.6",
    "glob-parent": "^6.0.2",
    "netlify-lambda": "^1.6.3",
    "postcss-cli": "^7.1.1",
    "svgstore-cli": "^2.0.0"
  },
  "browserlists": [
    "defaults",
    "not ie 11",
    "not ie_mob 11",
    "not baidu >= 7.12",
    "not and_qq >= 1.2",
    "not op_mini all"
  ]
}
